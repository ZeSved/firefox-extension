{
	"manifest_version": 2,
	"name": "No spoilers",
	"version": "1.0",

	"description": "Removes spoilers",

	"icons": {
		"48": "image.png"
	},

	"browser_action": {
		"default_popup": "/index.html",
		"default_icon": {
			"48": "image.png"
		},
		"browser_style": true
	},

	"content_scripts": [
		{
			"matches": ["https://www.youtube.com/*"],
			"js": ["d.js", "index.js"]
		}
	],

	"permissions": ["*://*.youtube.com/*", "storage", "tabs"],

	"web_accessible_resources": ["index.html", "index.js"]
}
